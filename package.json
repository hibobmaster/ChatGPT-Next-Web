    "js-yaml": "^4.1.1",
    "katex": "^0.16.27",
    "lodash-es": "^4.17.21",
    "markdown-to-txt": "^2.0.1",
    "mermaid": "^11.12.2",
    "nanoid": "^5.1.6",
    "next": "^16.0.8",
    "node-fetch": "^3.3.2",
    "openapi-client-axios": "^7.8.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
